<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>BLOG</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <header>
      <div class = "header-nav">
          
          
          <ul>
              <li class=""><a href="/list">BLOG</a></li>

          </ul>
      </div>

  </header>
    <main>
      <ul class="list">
        <% articles.forEach((article) => { %>
          <li>
            <h2><%= article.title %></h2>
            <p><%= article.summary %></p>
            <a href="/article/<%= article.id %>">続きを読む</a>
          </li>
        <% }) %>
      </ul>
    </main>
    <%- include('footer'); %>
  </body>
</html>